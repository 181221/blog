        <meta name="twitter:site" content="@kylemathews">
        <meta name="twitter:title" content="<%- @title %>">
        <meta name="twitter:description" content="<%- @summary %>">

        <!-- Open Graph data -->
        <meta property="og:title" content="<%- @title %>" />
